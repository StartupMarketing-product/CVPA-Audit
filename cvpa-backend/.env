# Server Configuration
PORT=3001
NODE_ENV=development

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/cvpa

# Redis
REDIS_URL=redis://localhost:6379

# JWT
JWT_SECRET=cvpa-development-secret-key-change-in-production
JWT_EXPIRES_IN=15m
JWT_REFRESH_SECRET=cvpa-development-refresh-secret-key-change-in-production
JWT_REFRESH_EXPIRES_IN=7d

# CORS
CORS_ORIGIN=http://localhost:3000

# API Configuration
ENABLE_REAL_APIS=true

# App Store (no API key needed, RSS feed is public)
APP_STORE_COUNTRY=us

# Yandex Maps API
YANDEX_MAPS_API_KEY=abc99673-3aaa-4e91-bb72-9030d59f9e0e

# Google Play (optional - will use scraping if not provided)
# GOOGLE_PLAY_SERVICE_ACCOUNT_KEY_PATH=

# Uzum (not yet implemented)
# UZUM_API_KEY=
